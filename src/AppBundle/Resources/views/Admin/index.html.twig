{% extends 'AppBundle::base-admin.html.twig' %}

{% block stylesheets %}
  {{ parent() }}
  {#
  {% stylesheets
  'bundles/plusnaomain/css/sales_ranking.css' filter='cssrewrite'
  %}
  <link href="{{ asset_url }}" rel="stylesheet" />
  {% endstylesheets %}
  #}
{% endblock %}

{% block javascripts %}
  {{ parent() }}
  {#
  {% javascripts
  '@PlusnaoMainBundle/Resources/public/js/sales_ranking.js'
  %}
  <script src="{{ asset_url }}"></script>
  {% endjavascripts %}
  #}
{% endblock %}

{% block body %}

  <div id="adminIndex" class="container">

    <div id="header">
      <div class="clearfix">
        <div class="pull-left">
          <h2>&nbsp;</h2>
        </div>
      </div>

      <div>
        <div v-bind:class="messageCssClass" v-text="message"></div>
      </div>
    </div>

    {% for type, flashMessages in app.session.flashbag.all() %}
      {% if type in ['info', 'success', 'notice', 'warning', 'danger'] %}
        <div>
          {% for flashMessage in flashMessages %}
            <div class="alert alert-{{ type }}">
              {{ flashMessage }}
            </div>
          {% endfor %}
        </div>
      {% endif %}
    {% endfor %}

  </div>

{% endblock %}
