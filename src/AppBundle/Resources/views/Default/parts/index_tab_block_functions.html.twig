                <th class="text-center">ラベル用CSV</th>
              </tr>
              <tr v-for="(index, data) in dataList">
                <td class="text-center" v-text="data.created"></td>
                <td class="text-center" v-text="data.username"></td>
                <td class="text-center" v-text="data.status_name"></td>
                <td class="text-center">
                　　<!-- ステータス2（集計済み）、3（確定ファイルアップロード済み）の場合、予定数ファイルダウンロード可能 -->
                  <a v-if="data.status == 2 || data.status == 3" v-bind:href="getDownloadUrl(data.id, 'planned', shippingDate)">
                    <i class="fa fa-download text-success"></i>
                    ダウンロード
                  </a>
                  <span v-else=>-</span>
                </td>
                <td class="text-center">
                  <span v-if="data.status == 2 && index == 0"><!-- アップロード出来るのは最新、かつ集計済みのみ -->
                    <input type="file" id="shoplistSpeedbinAcceptCsv" class="" accept=".csv">
                    <a v-bind:href="" v-on:click="uploadAcceptCsv(data.id)">
                      <i class="fa fa-upload text-success"></i>
                      アップロード
